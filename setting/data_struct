#This file define the data struct used in communication

Cookie (name "USER_COOKIE")
{
	value:md5(user_id)
	expired_time:DATE()
	game_id:int
}

login
{
	'username':char(32);
	'password':char(32);
}

register
{
	'username':char(32);
	'password':char(32);
}

register_result
{
	'username':char(32);
	'status':'success | error';
}

Game
{
	Gamd_id: INT
	P1: INT
	P2: INT
	P3: INT
}
